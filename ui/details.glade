<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface>
    <widget class="GtkWindow" id="window">
        <property name="default_width">500</property>
        <child>
            <widget class="GtkTable" id="table1">
                <property name="border_width">10</property>
                <property name="column_spacing">8</property>
                <property name="n_columns">2</property>
                <property name="n_rows">1</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkAlignment" id="left_alignment">
                        <property name="visible">True</property>
                        <property name="xalign">0.0</property>
                        <property name="yalign">0.0</property>
                        <property name="yscale">0.0</property>
                        <child>
                            <widget class="GtkVBox" id="vbox1">
                                <property name="spacing">10</property>
                                <property name="visible">True</property>
                                <property name="width_request">200</property>
                                <child>
                                    <widget class="GtkImage" id="image">
                                        <property name="file">/home/frank/Projects/AnimeList/pixmaps/question_mark.png</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="expand">False</property>
                                        <property name="fill">False</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkButton" id="mal_button">
                                        <property name="label" context="yes" translatable="yes">Go to MAL page</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkFrame" id="information">
                                        <property name="border_width">1</property>
                                        <property name="label" context="yes" translatable="yes">Information</property>
                                        <property name="label_yalign">0.0</property>
                                        <property name="shadow">none</property>
                                        <property name="shadow_type">none</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <placeholder/>
                                        </child>
                                    </widget>
                                    <packing>
                                        <property name="position">2</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkFrame" id="statistics">
                                        <property name="border_width">1</property>
                                        <property name="label" context="yes" translatable="yes">Statistics</property>
                                        <property name="label_yalign">0.0</property>
                                        <property name="shadow">none</property>
                                        <property name="shadow_type">none</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <placeholder/>
                                        </child>
                                    </widget>
                                    <packing>
                                        <property name="position">3</property>
                                    </packing>
                                </child>
                            </widget>
                        </child>
                    </widget>
                </child>
                <child>
                    <widget class="GtkAlignment" id="right_alignment">
                        <property name="visible">True</property>
                        <property name="xalign">0.0</property>
                        <property name="yalign">0.0</property>
                        <property name="yscale">0.0</property>
                        <child>
                            <widget class="GtkVBox" id="vbox2">
                                <property name="spacing">10</property>
                                <property name="visible">True</property>
                                <child>
                                    <widget class="GtkLabel" id="title">
                                        <property name="can_focus">True</property>
                                        <property name="label" context="yes" translatable="yes">Title</property>
                                        <property name="selectable">True</property>
                                        <property name="visible">True</property>
                                        <property name="wrap">True</property>
                                        <property name="xalign">0.0</property>
                                        <property name="yalign">0.0</property>
                                    </widget>
                                </child>
                                <child>
                                    <widget class="GtkHSeparator" id="hseparator1">
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkScrolledWindow" id="scrolledwindow1">
                                        <property name="height_request">150</property>
                                        <property name="hscrollbar_policy">never</property>
                                        <property name="visible">True</property>
                                        <property name="vscrollbar_policy">automatic</property>
                                        <child>
                                            <widget class="GtkViewport" id="viewport1">
                                                <property name="shadow_type">none</property>
                                                <property name="visible">True</property>
                                                <child>
                                                    <widget class="GtkLabel" id="synopsis">
                                                        <property name="can_focus">True</property>
                                                        <property name="label" context="yes" translatable="yes">Synopsis</property>
                                                        <property name="selectable">True</property>
                                                        <property name="visible">True</property>
                                                        <property name="wrap">True</property>
                                                        <property name="xalign">0.0</property>
                                                        <property name="yalign">0.0</property>
                                                    </widget>
                                                </child>
                                            </widget>
                                        </child>
                                    </widget>
                                    <packing>
                                        <property name="position">2</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkFrame" id="other_titles">
                                        <property name="label" context="yes" translatable="yes">Other titles</property>
                                        <property name="shadow">none</property>
                                        <property name="shadow_type">none</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <placeholder/>
                                        </child>
                                    </widget>
                                    <packing>
                                        <property name="position">3</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkFrame" id="related">
                                        <property name="label" context="yes" translatable="yes">Related anime/manga</property>
                                        <property name="shadow">none</property>
                                        <property name="shadow_type">none</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <placeholder/>
                                        </child>
                                    </widget>
                                    <packing>
                                        <property name="position">4</property>
                                    </packing>
                                </child>
                            </widget>
                        </child>
                    </widget>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
